{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "array-type": [
      false
    ],
    "arrow-parens": false,
    "eofline": true,
    "max-classes-per-file": true,
    "max-line-length": [
      true,
      200
    ],
    "member-access": [
      "check-accessor"
    ],
    "object-literal-sort-keys": [
      false
    ],
    "one-line": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      "allow-leading-underscore"
    ],
    "no-default-export": true,
    "no-empty-interface": false,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-submodule-imports": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  }
}
